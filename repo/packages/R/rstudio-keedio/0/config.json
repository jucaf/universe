{
  "type": "object",
  "properties": {
    "service": {
      "type": "object",
      "description": "DC/OS service configuration properties",
      "properties": {
        "name": {
          "description": "rstudio from The Rocker Project",
          "type": "string",
          "default": "rstudio-keedio"
        },
        "cpus": {
          "description": "rstudio pod CPU requirements",
          "type": "integer",
          "default": 1
        },
        "mem": {
          "description": "rstudio pod mem requirements (in MB)",
          "type": "integer",
          "default": 128 
        },
	"password": {
          "description": "rstudio password",
          "type": "string",
          "default": "keedio"
        }
      }
    }
  }
}
